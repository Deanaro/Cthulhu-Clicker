//takes a string and removes any present underscore

//create variables
str = argument[0];

//remove under scores from titles
underscores = 1
while (underscores = 1)
{
    if string_pos('_', str) != 0
    {
    str = string_replace(str, '_', ' ');
    }
    else
    {
    underscores = 0;
    }
}

return (str);
