        <arguments>
          <argument>
            <kind>1</kind>
            <string>///check is the game has been minimised
//lachlan

if os_is_paused()
    {
    alarm [4] = 1
    scr_save();
    }

if global.view_lock
{    
//view swipe functionality
if ui_swipe_start = 1   //if the view is being swiped, offset view to match the mouse offset
    {
        mouse_previous = temp_mouse_pos;
        temp_mouse_pos = mouse_x;
        change_in_x = ceil((mouse_x - view_xview)- mouse_view_x);
        view_xview = ui_view_pos - change_in_x;
        x = view_xview;
    }
else if ui_swipe_snap = 1   //when mouse is swipe ends, snaps to the closest view
    {
        if view_xview/view_wview - floor(view_xview/view_wview) &gt;= 0.5
        {closest_view_dir = 1;}
        else 
        {closest_view_dir = -1;}
        closest_view = round(view_xview / view_wview)
        
        if closest_view &lt; index_minView {closest_view = index_minView; closest_view_dir = 1; if swipe_momentom &lt; 0 {swipe_momentom = 0;}}
        else if closest_view &gt; index_maxView {closest_view = index_maxView; closest_view_dir = -1;if swipe_momentom &gt; 0 {swipe_momentom = 0;}}
        
        if abs(round(swipe_momentom)) &gt;= abs(closest_view*view_wview-view_xview)
            {
            ui_swipe_snap=0;
            view_xview = closest_view*view_wview;
            }
        else
            {
            swipe_momentom += closest_view_dir;
