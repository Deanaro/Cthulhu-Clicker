    obj_control.alarm[6] = 1;
    obj_control.buff_time = time;
    obj_control.buff_amount = bonus;
    //reset pass counter
    obj_control.buff_timer_pass_count = 0;
    
    //create text to display in pop up
    buff_text = "you get " +string(bonus * 100) +"% insanity gain 
    for the next " +string(time)+ " seconds.";
    }
else
    {
    //determine insanity amount
    insanity_gain = round(obj_control.afflicted * random_range(0.1,0.4));
    
    //add insanity to players pool
    obj_control.insanity = obj_control.insanity + insanity_gain;
    
    //create text to display in pup up
    buff_text = "you get " +string(insanity_gain) +" insanity!";
    }
    
