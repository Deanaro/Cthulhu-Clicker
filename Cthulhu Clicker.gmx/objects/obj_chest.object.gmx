            <string>///choose buff
//lachlan
//choose is buff is insanity or timed buff
if (clicked = 1) {
if (irandom_range(0,10) &lt;= 4)
    {
    //determine timed buff
    buff = irandom_range(0,4)
    
    //determine times amount of dps done in time
    switch (buff)
    {
    case 0: bonus = 1.25; break
    case 1: bonus = 1.50; break
    case 2: bonus = 2; break
    case 3: bonus = 3; break
    case 4: bonus = 4; break
    }
    
    //determine time buff is active for in seconds
